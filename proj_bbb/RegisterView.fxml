         <Label fx:id="label_status" layoutX="425.0" layoutY="364.0" prefHeight="17.0" prefWidth="155.0" text="status" />
         <ImageView fitHeight="80.0" fitWidth="100.0" layoutX="23.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@itc_logo.png" />
